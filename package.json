{
    "name": "gitfiles",
    "version": "1.0.0",
    "description": "Web app using Github's API to display information on Github Users",
    "main": "index.js",
    "license": "MIT",
    "scripts": {
        "start": "webpack-dev-server --progress --inline --hot",
        "test": "jest --colors"
    },
    "jest": {
        "setupFiles": [
            "raf/polyfill",
            "<rootDir>/utils/jest/setup.js"
        ],
        "moduleFileExtensions": [
            "js",
            "jsx"
        ],
        "testPathIgnorePatterns": [
            "/node_modules/"
        ],
        "moduleNameMapper": {
            "^app(.*)$": "<rootDir>/app/$1",
            "^utils(.*)$": "<rootDir>/utils/$1",
            "^components(.*)$": "<rootDir>/app/components/$1"
        }
    },
    "devDependencies": {
        "babel-eslint": "^7.2.1",
        "babel-jest": "^21.2.0",
        "babel-plugin-styled-components": "^1.3.0",
        "babel-polyfill": "^6.26.0",
        "babel-preset-env": "^1.6.1",
        "babel-preset-stage-2": "^6.24.1",
        "chai": "^4.1.2",
        "css-loader": "^0.28.7",
        "enzyme": "^3.1.1",
        "enzyme-adapter-react-16": "^1.0.4",
        "eslint-plugin-jsx-a11y": "^4.0.0",
        "jest": "^21.2.1",
        "prettier": "^1.8.2",
        "regenerator-runtime": "^0.11.0",
        "sinon": "^4.1.2",
        "style-loader": "^0.19.0",
        "webpack": "^3.8.1",
        "webpack-dev-server": "^2.9.4"
    },
    "dependencies": {
        "add": "^2.0.6",
        "babel-cli": "^6.26.0",
        "babel-core": "^6.26.0",
        "babel-loader": "^7.1.2",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-react": "^6.24.1",
        "downshift": "^1.17.0",
        "eslint": "^3.19.0",
        "eslint-plugin-react": "^6.10.3",
        "immutable": "^3.8.2",
        "normalize.css": "^7.0.0",
        "prop-types": "^15.6.0",
        "react": "^16.1.0",
        "react-dom": "^16.1.0",
        "react-icons": "^2.2.7",
        "react-immutable-proptypes": "^2.1.0",
        "rxjs": "^5.5.2",
        "styled-components": "^2.2.3"
    }
}
